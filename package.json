{
  "name": "gatsby-minimal-portfolio-airtable",
  "private": false,
  "description": "Template for a personal blog",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "type-check": "tsc --noEmit",
    "lint": "./node_modules/.bin/eslint --ext .js,.jsx,.ts,.tsx src/",
    "clean": "gatsby clean",
    "prepare": "husky install",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "lint-staged": {
    "src/*": [
      "yarn format",
      "yarn type-check"
    ],
    "*.{js,jsx,ts,tsx,json,md}": "prettier --write"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "dotenv": "^10.0.0",
    "gatsby": "^3.10.1",
    "gatsby-plugin-feed": "^4.3.0",
    "gatsby-plugin-google-fonts": "^1.0.1",
    "gatsby-plugin-image": "^1.12.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-sharp": "^3.12.0",
    "gatsby-remark-autolink-headers": "^5.3.0",
    "gatsby-remark-copy-linked-files": "^5.3.0",
    "gatsby-remark-external-links": "0.0.4",
    "gatsby-remark-prismjs": "^6.3.0",
    "gatsby-remark-smartypants": "^5.3.0",
    "gatsby-source-airtable": "^2.2.0",
    "gatsby-source-filesystem": "^3.10.0",
    "gatsby-transformer-sharp": "^3.12.0",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.25.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-seo-component": "^2.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.27.1",
    "gatsby-plugin-typescript": "^3.11.0",
    "gatsby-remark-images": "^5.10.0",
    "gatsby-remark-images-anywhere": "^1.3.1",
    "gatsby-transformer-remark": "^4.10.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "2.3.2",
    "typescript": "^4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Alexandrshy/gatsby-minimal-portfolio-airtable"
  },
  "author": "Alex Shulaev <alexandrshy@gmail.com>",
  "bugs": {
    "url": "https://github.com/Alexandrshy/gatsby-minimal-portfolio-airtable/issues"
  }
}
